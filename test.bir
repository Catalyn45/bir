module main

implement T {
    function get_a(): int {
        return this.a
    }
}

function main(): int {
	var t = T(15)
    var n = T(25)

    t.next = n

    t = t.next

    return t.get_a()
}

implement T {
	init(a: int) {
        this.a = a
	}
}

struct T {
    a: int
    b: float
    next: T
}

