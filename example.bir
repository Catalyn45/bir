module main

import std

interface Displayable {
    function show()
}

struct Test {
    a: int
    b: int
}

implement Test {
    function init(a: int, b: int) {
        this.a = a
        this.b = b
    }

    function show() {
        print(a + " " + b)
    }

    function add(): int {
        return a + b
    }
}

function showMe(s: Displayable) {
    s.show()
}

function main() {
    var t = Test(10, 20)

    var sum: int = t.add()

    showMe(t)
}